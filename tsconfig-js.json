{
  "compilerOptions": {
    "strict": false,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es2019",
    "allowJs": true,
    "checkJs": false,
    "outDir": "generated",
    "noEmitOnError": false,
    "declaration": false
  },
  "exclude": [
    "node_modules",
    "../src/test/**/*",
    "../src/**/*_test.js",
    "../src/keyboard/vim.js",
    "../src/keyboard/emacs.js",
    "../src/keyboard/sublime.js",
    "../src/keyboard/vscode.js",
    "../src/mode/**/*",
    "../src/snippets/**/*",
    "../src/theme/**/*",
    "../src/**/*.d.ts"
  ],
  "include": [
    "../src/**/*.js",
    "../interfaces.d.ts",
    "../ace-internal.d.ts"
  ],
  "typedocOptions": {
    "name": "Ace API Reference",
    "readme": "assets/ace_readme.md",
    "excludeReferences": true,
    "sourceLinkExternal": true,
    "entryPoints": [
      "../src/**/*.js",
      "../interfaces.d.ts"
    ],
    "exclude": [
      "node_modules",
      "../src/test/**/*",
      "../src/**/*_test.js",
      "../src/keyboard/vim.js",
      "../src/keyboard/emacs.js",
      "../src/keyboard/sublime.js",
      "../src/keyboard/vscode.js",
      "../src/mode/**/*",
      "../src/snippets/**/*",
      "../src/theme/**/*",
      "../src/**/*.d.ts"
    ]
  }
}
